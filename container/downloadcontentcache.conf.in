
    proxy_cache_path  /var/cache/nginx  levels=1:2    keys_zone=STATIC:20m
    inactive=2400h  max_size={{storagesize}} use_temp_path=off;
    server {
        listen       80;
        server_name  localhost;
        location / {
            proxy_pass             {{backendserver}};
            #proxy_set_header       Host $host;
            proxy_buffering        on;
            proxy_socket_keepalive on;
            proxy_ssl_session_reuse on;
            proxy_ssl_verify       on;
            proxy_ssl_verify_depth 3;
            proxy_ssl_trusted_certificate /var/lib/ca-certificates/ca-bundle.pem;
            proxy_ssl_protocols    TLSv1.2 TLSv1.3;
            proxy_cache_revalidate on;
            proxy_http_version     1.1;
            proxy_cache            STATIC;
            proxy_cache_valid      200  {{ttl}};
            proxy_cache_valid      any  1m;
            proxy_cache_use_stale  error timeout invalid_header updating
                                   http_500 http_502 http_503 http_504;
        }

        #location /.well-known/acme-challenge {
        #    proxy_pass http://[fd4c:ab24:3f04::16]:8080;
        #}
        #ssl_certificate /etc/letsencrypt/live/downloadcontent2.opensuse.org/fullchain.pem;
        #ssl_certificate_key /etc/letsencrypt/live/downloadcontent2.opensuse.org/privkey.pem;
    }

